class Node:
  def __init__(self,value):
    #value,left,right
    self.val=value
    self.left=None
    self.right=None
#Example of Binary tree
root=Node(1)
root.left=Node(2)
root.right=Node(3)
root.left.left=Node(4)
root.left.right=Node(5)
def preorder(root):
  if root is None:
    return None
  print(root.val,end="")
  preorder(root.left)
  preorder(root.right)
preorder(root)
def inorder(root):
  if root is None:
    return None
  
  inorder(root.left)
  print(root.val,end="")
  inorder(root.right)
inorder(root)
