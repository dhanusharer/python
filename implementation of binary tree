class Node:
  def __init__(self,value):
    #value,left,right
    self.val=value
    self.left=None
    self.right=None
#Example of Binary tree
root=Node(1)
root.left=Node(2)
root.right=Node(3)
root.left.left=Node(4)
root.left.right=Node(5)

def display(root):
   if root is None:
       return None
   print(root.val)
   display(root.left)
   display(root.right)


display(root)